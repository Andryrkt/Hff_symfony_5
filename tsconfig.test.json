{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "types": [
            "jest",
            "node"
        ],
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "assets/**/__tests__/**/*",
        "assets/**/*.test.ts",
        "assets/**/*.test.js",
        "assets/**/*.spec.ts",
        "assets/**/*.spec.js"
    ]
}