{
    "compilerOptions": {
        "target": "es6",
        "module": "esnext",
        "lib": ["dom", "es6"],
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "allowJs": true,
        "noEmit": false,
        "resolveJsonModule": true
    },
    "include": [
        "assets/**/*.ts",
        "assets/**/*.js"
    ],
    "exclude": [
        "node_modules"
    ]
}
